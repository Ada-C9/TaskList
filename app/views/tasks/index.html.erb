<h1 class=page-title>Task List</h2>
<section class="task-summary">
  <h2>Your Tasks</h2>
  <ol>
    <% @tasks.each do |task| %>
    <li class="task">
      <span class="title">
        <%= link_to task.description, task_path(task.id) %>
      </span>
    </li>
    <%end%>
  </ol>
</section>

